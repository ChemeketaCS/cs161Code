#include <iostream>

using namespace std;

int main() {
  int i = 1, j = 1;
  while(i <= 10) {
    // j only has value of 1 the first time through...
    while(j <= 10) {
      cout << i << "-" << j << "\t";
      j++;
    } // end of inner loop

    cout << endl;
    i++;
  } // end of outer loop
}
