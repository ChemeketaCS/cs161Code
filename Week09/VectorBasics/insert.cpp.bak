#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main() {
  vector<char> letters = {'c', 'f'};
  letters.insert(letters.begin(), 'a'); // insert 'a' at the beginning
  // now letters is {'a', 'c', 'f'}
  letters.insert(letters.begin() + 1, 'b'); // insert 'b' at index 1
  // now letters is {'a', 'b', 'c', 'f'}
  letters.insert(letters.begin() + 3, 'd'); // insert 'd' at index 3
  // now letters is {'a', 'b', 'c', 'd', 'f'}
  letters.insert(letters.end() - 1, 'e'); // insert 'e' before the last element

  // print all the letters - covered in the next section
  for (char c : letters) {
    cout << c << " ";
  }
}
