#include <iostream>
#include <vector>
using namespace std;

// total up all the values in numbers
int total(vector<int> numbers) {
    int total = 0;
    for (int x : numbers) {
        total += x;
    }
    return total;
}

int main() {
    vector<int> myList = {1, 2, 3, 4};
    
    int sum = total(myList);
    cout << "Total: " << sum << endl;
}
