#include <iostream>
using namespace std;

int main() {
  int x = 1;
  int y = 0;

  if (x == 0) {
    if (y == 0) {
        cout << "Both x and y are zero";
    }
  }

  if (x == 0 && y == 0) {
      cout << "Both x and y are zero";
  }

  if (x == 3 && y >= 0 && y <= 10) {
      cout << "x is 3 and y is between 0 and 10";
  }
}
